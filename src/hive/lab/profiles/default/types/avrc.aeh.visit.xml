<?xml version="1.0"?>
<object name="avrc.aeh.visit"
   meta_type="Dexterity FTI"
   i18n:domain="avrc.aeh" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
    <property name="behaviors" purge="false">
        <element value="hive.lab.interfaces.specimen.IRequestedSpecimen" />
        <element value="hive.lab.interfaces.aliquot.IAliquotSupport"/>
    </property>
    <alias from="specimen" to="@@specimen" />
    <alias from="aliquot" to="@@aliquot" />

    <action title="Specimen" action_id="specimen" category="object" condition_expr=""
    url_expr="string:${object_url}/specimen" visible="True">
        <permission value="hive: Request Specimen" />
    </action>
    <action title="Aliquot" action_id="aliquot" category="object" condition_expr=""
    url_expr="string:${object_url}/aliquot" visible="True">
        <permission value="hive: View Aliquot" />
    </action>    
</object>
