<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="hive.lab"
      xml:lang="en"
      lang="en">
  <metal:content-core fill-slot="content-core">
    <metal:content-core define-macro="content-core">
        <tal:data define="form view/aliquotque">
            <tal:hasdata tal:condition="form">
                Description of Form Here
                <tal:crud replace="structure form" />
                <br class="visualClear" />
                Description of buttons here
            </tal:hasdata>
            <tal:nodata tal:condition="not:form">
                No Data 
            </tal:nodata>
        </tal:data>
        <hr class="visualClear" />
        <tal:data define="form view/formhelper">
            <tal:hasdata tal:condition="form">
                Description of Form Here
                <tal:crud replace="structure form" />
                <br class="visualClear" />
                Description of buttons here
            </tal:hasdata>
            <tal:nodata tal:condition="not:form">
                No Data 
            </tal:nodata>
        </tal:data>
        <hr class="crudform" />
        <tal:data define="form view/crudform">
            <tal:hasdata tal:condition="form">
                Description of Form Here
                <tal:crud replace="structure form" />
                <br class="visualClear" />
                Description of buttons here
            </tal:hasdata>
            <tal:nodata tal:condition="not:form">
                No Data 
            </tal:nodata>
        </tal:data>
        <br class="visualClear" />

    </metal:content-core>
  </metal:content-core>
</html>
    
